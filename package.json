    "@tsed/json-mapper": "^7.83.3",
    "@tsed/logger": "^6.7.8",
    "@tsed/logger-file": "^6.7.8",
    "@tsed/openspec": "^7.83.3",
    "@tsed/platform-cache": "^7.83.3",
    "@tsed/platform-exceptions": "^7.83.3",
    "@tsed/platform-express": "^7.83.3",
