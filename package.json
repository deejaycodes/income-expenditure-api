    "@tsed/schema": "^7.83.3",
    "@tsed/swagger": "^7.83.3",
    "ajv": "^8.17.1",
    "barrelsby": "^2.8.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
